{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
  "rules": {
    "indent": ["error", 4],
    "linebreak-style": ["error", "unix"],
    "comma-dangle": ["error", "never"],
    "no-restricted-syntax": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "lines-between-class-members": 0
  }
}
